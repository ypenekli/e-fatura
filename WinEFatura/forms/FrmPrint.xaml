<Window x:Class="com.yp.efatura.forms.FrmPrint"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:com.yp.efatura.forms"
        mc:Ignorable="d"
        Title="FATURA GÖRÜNTÜLEME VE YAZDIRMA" Height="450" Width="800">
    <DockPanel LastChildFill="True" >
        <ToolBarPanel DockPanel.Dock="Top" VerticalAlignment="Top">
            <ToolBar Margin="0,0,5,0">
                <Button x:Name="btnPrint1" Content="YAZDIR" Click="btnPrint1_Click"  />
                <Separator/>
                <Button x:Name="btnPrint2" Content="ÖN İZLEME" Click="btnPrint2_Click" />
            </ToolBar>
        </ToolBarPanel>
        <WebBrowser x:Name="webbrowser"  DockPanel.Dock="Top"/>

    </DockPanel>
</Window>
